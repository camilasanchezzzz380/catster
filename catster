	  {
		if(ale_on_click_checkbox_is_checked) {
			Georiot.amazon.addOnClickRedirect(348989, false);
		}
		else {
			Georiot.amazon.convertToGeoRiotLinks(348989, false);
		};
	  };
    });
  </script>
